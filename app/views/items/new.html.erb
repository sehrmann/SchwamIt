<h1>Submit a new 90s thing</h1>

<%= form_for(@item) do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>
  <div>
    <%= f.file_field :image, required: true %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
